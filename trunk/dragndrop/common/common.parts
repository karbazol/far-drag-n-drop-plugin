Import('*')

env.PartName('common')
env.PartVersion('2.0.0')

sources = env.Glob('*.cpp') + env.Glob('*.c')
headers = env.Glob('*.hpp') + env.Glob('*.h')

env.SdkInclude(headers)
env.SdkLib(env.StaticLibrary(target='${PART_NAME}_${TARGET_ARCH}', source=sources))

