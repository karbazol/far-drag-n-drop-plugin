Import('*')

env.PartName('common')
env.PartVersion('2.0.0')

sources = Pattern(includes=['*.cpp', '*.c'], recursive=False).files()
headers = Pattern(includes=['*.h', '*.hpp'], recursive=False).files()

env.SdkInclude(headers)
env.SdkLib(env.StaticLibrary(target='common', source=sources))

